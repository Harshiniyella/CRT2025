NESTED LOOP
Print A  B  C  D  E 
      A  B  C  D  E 
      A  B  C  D  E 
      A  B  C  D  E 
      A  B  C  D  E 

#include <stdio.h>

int main()
{
    int i,j;
    for(j=1;j<=5;j++)
    {
        for(i=1;i<=5;i++)
        {
            printf(" %c ",64+i);
        }
        printf("\n");
    }
    return 0;
}
