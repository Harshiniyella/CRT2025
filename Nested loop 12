NESTED LOOP
Print  a  b  c  d  e 
       a  b  c  d  e 
       a  b  c  d  e 
       a  b  c  d  e 
       a  b  c  d  e 

#include <stdio.h>

int main()
{
    int i,j;
    for(j=1;j<=5;j++)
    {
        for(i=1;i<=5;i++)
        {
            printf(" %c ",96+i);
        }
        printf("\n");
    }
    return 0;
}
