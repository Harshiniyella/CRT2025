Jumping statement
#include <stdio.h>

int main()
{
    int n,i,c=0;
    scanf("%d",&n);
    for(i=2;i<n;i++)
    {
       if(n%i==0)
       {
           c=1;
           break;
       }
    }
    printf("%d\n",i);
    if(c==0 && n!=1)
    printf("Prime no");
    else
    printf("Not a Prime no");
    
  return 0;  
}
